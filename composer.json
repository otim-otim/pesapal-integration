{
    "name": "otim-otim/pesapal-laravel-integration",
    "description": "This package integrates pesapal and its functionality to a laravel application",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "OtimOtim\\PesapalIntegrationPackage\\": "src/",
            "OtimOtim\\PesapalIntegrationPackage\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Otim Isaac",
            "email": "otim.idb@gmail.com"
            
        }
